{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "main"
  },
  "defaultBase": "main",
  "nxCloudAccessToken": "ZWJiNDliYTgtNzFiMi00NDg2LWFkZGMtYjIxOWY0MDU1ZGYxfHJlYWQtb25seQ==",
  "parallel": 5,
  "namedInputs": {
    "sharedGlobals": [
      "{workspaceRoot}/.eslintrc.cjs",
      "{workspaceRoot}/.nvmrc",
      "{workspaceRoot}/package.json",
      "{workspaceRoot}/tsconfig.json"
    ],
    "default": [
      "sharedGlobals",
      "{projectRoot}/**/*",
      "!{projectRoot}/**/*.md"
    ],
    "public": [
      "default",
      "{projectRoot}/build",
      "{projectRoot}/dist",
      "!{projectRoot}/.eslintrc.cjs",
      "!{projectRoot}/tsconfig.eslint.json"
    ]
  },
  "targetDefaults": {
    "test:unit": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^public"],
      "outputs": ["{projectRoot}/coverage"],
      "cache": true
    },
    "test:eslint": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^public"],
      "cache": true
    },
    "test:types": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^public", { "env": "TS_VERSION" }],
      "cache": true
    },
    "test:build": {
      "dependsOn": ["build"],
      "inputs": ["default", "^public"],
      "cache": true
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^public"],
      "outputs": ["{projectRoot}/build", "{projectRoot}/dist"],
      "cache": true
    },
    "test:format": {
      "cache": true,
      "inputs": ["{workspaceRoot}/**/*"]
    }
  }
}
